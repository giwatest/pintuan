CREATE TABLE IF NOT EXISTS `goods`(
   `goods_id` INT UNSIGNED AUTO_INCREMENT,
   `info_img` VARCHAR(320) NOT NULL,
   `goods_name` VARCHAR(320) NOT NULL,
   `group_person` INT NOT NULL,
   `goods_sale_num` INT NOT NULL,
   `goods_num` INT NOT NULL,
   `goods_now_price` Float NOT NULL,
   `goods_price` Float NOT NULL,
   `goods_low_price` Float NOT NULL,
   `goods_brief` TEXT NOT NULL,
   PRIMARY KEY ( `goods_id` )
)ENGINE=InnoDB DEFAULT CHARSET=utf8;